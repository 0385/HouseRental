/* components/moreMenu/menu.wxss */




.menu{
    width: 100%;
    background-color: #fff;
    margin-top: 25px;
}

.menu-item{
    width: 680rpx;
    height: 30px;
    margin: 0 auto;
    border-bottom: 1px solid #ccc;
    padding: 10px 10px 0px 5px;
    position: relative;
}
.items{
    /* border: 1px solid red; */
    display: inline-block;
    padding: 3px;
}
.iconfont{
    margin: 0px 5px 7px;
}
.arrow{
  display: inline-block;
  position: absolute;
  /* border:1px solid #ccc; */
  /* margin-left: 200px; */
  /* margin-right: 50px; */
  left: 300px;
}




